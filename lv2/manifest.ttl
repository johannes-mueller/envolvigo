
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .

@prefix $PLUGINNAME$: <https://johannes-mueller.org/lv2/$PLUGINNAME$#> .

$PLUGINNAME$:lv2
	a lv2:Plugin ;
 	lv2:binary <lib$PLUGINNAME$.so> ;
	rdfs:seeAlso <$PLUGINNAME$.ttl> .

$PLUGINNAME$:ui
	a ui:X11UI ;
	ui:binary <lib$PLUGINNAME$_ui.so> ;
	rdfs:seeAlso <$PLUGINNAME$.ttl> .
